{
  "name": "deps-ok",
  "description": "Fast checking of top level dependencies based on version numbers",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bin": "index.js",
  "version": "0.0.0-semantic-release",
  "bugs": {
    "url": "https://github.com/bahmutov/deps-ok/issues"
  },
  "contributors": [
    "Tilman Potthof"
  ],
  "dependencies": {
    "check-types": "1.4.0",
    "lodash": "3.10.1",
    "optimist": "0.6.1",
    "q": "2.0.3",
    "quote": "0.4.0",
    "semver": "5.1.0"
  },
  "devDependencies": {
    "git-issues": "1.2.0",
    "grunt": "0.4.5",
    "grunt-contrib-jshint": "0.11.3",
    "grunt-nice-package": "0.9.6",
    "gt": "0.8.48",
    "jshint-summary": "0.4.0",
    "pre-git": "1.4.0",
    "semantic-release": "^4.3.5"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "gitHead": "a46441093a60dc95720c7b333d0a64f7ab0749b4",
  "homepage": "https://github.com/bahmutov/deps-ok",
  "keywords": [
    "npm",
    "install",
    "dependencies",
    "package",
    "version"
  ],
  "license": "MIT",
  "main": "index.js",
  "preferGlobal": true,
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/deps-ok.git"
  },
  "files": [
    "index.js",
    "src",
    "bin"
  ],
  "scripts": {
    "test": "npm run itself && npm run unit && npm run e2e",
    "itself": "node index.js",
    "unit": "node ./node_modules/gt src/test/**/*.coffee --output",
    "e2e": "node ./node_modules/gt test/*.coffee --output",
    "commit": "git-issues && commit-wizard",
    "issues": "git-issues",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "npm test"
      ],
      "pre-push": [
        "npm run size"
      ],
      "post-commit": [],
      "post-merge": []
    }
  }
}
